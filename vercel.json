{
    "version": 2,
    "functions": {
        "/": {
            "maxDuration": 20,
            "runtime": "@vercel/node"
        }
      },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.ts",
            "methods": [
                "GET",
                "POST",
                "DELETE"
            ]
        }
    ]
}