{
    "version": 2,
    "functions": {
        "maxDuration": 20,
        "runtime": "@vercel/node",
        "src": "index.ts"
      },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.ts",
            "methods": [
                "GET",
                "POST",
                "DELETE"
            ]
        }
    ]
}