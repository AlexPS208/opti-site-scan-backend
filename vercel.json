{
    "version": 2,
    "functions": {
      "api/index": {
        "maxDuration": 10,
        "runtime": "@vercel/node"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.ts",
        "methods": [
          "GET",
          "POST",
          "DELETE"
        ]
      }
    ]
  }