{
    "version": 2,
    "functions": {
        "/": {
          "maxDuration": 10
        }
      },
    "builds": [
        {
            "src": "index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.ts",
            "methods": [
                "GET",
                "POST",
                "DELETE"
            ]
        }
    ]
}