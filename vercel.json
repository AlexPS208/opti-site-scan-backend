{
    "version": 2,
    "builds": [
        {
            "src": "index.ts",
            "use": "@vercel/node",
            "config": {
              "maxDuration": 60
            }
        }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.ts",
        "methods": [
          "GET",
          "POST"
        ]
      }
    ],
    "functions": {
        "maxDuration": 60
    }
  }