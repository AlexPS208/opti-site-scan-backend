{
    "version": 2,
    "functions": {
      "api/*.js": {
        "maxDuration": 20,
        "runtime": "@vercel/node@20.9.0"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.ts",
        "methods": [
          "GET",
          "POST",
          "DELETE"
        ]
      }
    ]
  }