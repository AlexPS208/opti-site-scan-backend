{
    "version": 2,
    "builds": [
        {
            "src": "index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.ts",
            "methods": [
                "GET",
                "POST",
                "DELETE"
            ]
        }
    ],
    "functions": {
        "index": {
          "maxDuration": 20
        }
      }
}